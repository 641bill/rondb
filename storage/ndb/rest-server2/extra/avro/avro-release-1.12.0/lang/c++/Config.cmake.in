@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

find_dependency(Boost REQUIRED)

# Only find Snappy if it was actually used during the build
if(@SNAPPY_FOUND@)
    find_dependency(Snappy)
endif()

find_dependency(fmt REQUIRED)

include("${CMAKE_CURRENT_LIST_DIR}/avrocppTargets.cmake")

set(AvroCpp_INCLUDE_DIRS "@PACKAGE_INCLUDE_INSTALL_DIR@")
set(AvroCpp_LIBRARIES avrocpp)

set(AvroCpp_INCLUDE_DIRS "${AvroCpp_INCLUDE_DIRS}" PARENT_SCOPE)
set(AvroCpp_LIBRARIES "${AvroCpp_LIBRARIES}" PARENT_SCOPE)
