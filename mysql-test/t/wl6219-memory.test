########################################
##### WL#7554 Force schema `test` to character set latin1
##### This test should be migrated to utf8mb4
--disable_query_log
let $testdbcs316=`select default_character_set_name from information_schema.schemata where schema_name='test'`;
drop schema test;
create schema test character set latin1;
use test;
--enable_query_log
########################################

#
# WL#6219: Deprecate and remove YEAR(2) type
#

let $engine_type= MEMORY;
--source include/wl6219-engine.test

########################################
##### WL#7554 set schema `test` back to what it was
--disable_query_log
drop schema test;
--eval create schema test character set $testdbcs316;
--enable_query_log
########################################
