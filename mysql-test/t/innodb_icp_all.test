########################################
##### WL#7554 Force schema `test` to character set latin1
##### This test should be migrated to utf8mb4
--disable_query_log
let $testdbcs197=`select default_character_set_name from information_schema.schemata where schema_name='test'`;
drop schema test;
create schema test character set latin1;
use test;
--enable_query_log
########################################


set optimizer_switch='semijoin=on,materialization=on,firstmatch=on,loosescan=on,index_condition_pushdown=on,mrr=on';


set @save_storage_engine= @@default_storage_engine;
set default_storage_engine=InnoDB;

--source include/icp_tests.inc

set default_storage_engine= @save_storage_engine;
set optimizer_switch=default;

########################################
##### WL#7554 set schema `test` back to what it was
--disable_query_log
drop schema test;
--eval create schema test character set $testdbcs197;
--enable_query_log
########################################
