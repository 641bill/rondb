--source include/have_ndb.inc

create table t (c varchar(20),d varchar(20)) engine=ndb;
insert into t values ('\b','backspace');
select sum(char_length(c)) from t;
insert into t values (X'10','data link escape');                   # needs \u escape
select sum(char_length(c)) from t;

select * from t order by c;
drop table t;
