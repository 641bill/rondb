!include suite/ndb/my.cnf

[cluster_config.1]

# 'Job buffer full' become more likey when not communicating
# between nodes, having just a few job buffer 'channels'
#ndbd =
#ThreadConfig=ldm={count=1},tc={count=1} #,recv={count=1}
MaxNoOfExecutionThreads=4

# Scale up for handling larger transactions:
DataMemory = 256M
NoOfReplicas = 1
MaxNoOfConcurrentOperations = 512K
NoOfFragmentLogFiles = 16
