use mysql;
#
# Subset of definitions from tables, not including
# timestamps, partitioning, view definitions and
# default values
SELECT
id, name, type, engine, collation_id, comment, hidden,
options, se_private_data, se_private_id, tablespace_id
FROM tables
WHERE id IN (SELECT id FROM tables
WHERE schema_id= 1
AND name IN ('innodb_table_stats',
'innodb_index_stats',
'character_sets',
'collations',
'tablespaces',
'tablespace_files',
'catalogs',
'schemata',
'tables',
'view_table_usage',
'columns',
'indexes',
'index_column_usage',
'column_type_elements',
'foreign_keys',
'foreign_key_column_usage',
'table_partitions',
'table_partition_values',
'index_partitions',
'version'))
ORDER BY id;
